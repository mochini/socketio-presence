{
  "name": "socketio-presence",
  "version": "1.0.0",
  "description": "User presence experiment",
  "main": "index.js",
  "scripts": {
    "build:server": "rm -rf ./dist && babel ./src --out-dir=./dist",
    "build:client": "rm -rf ./public && cp -rp ./src/public ./public && webpack",
    "build": "npm run build:server && npm run build:client",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mochini/socketio-presence.git"
  },
  "author": "",
