  "bugs": {
    "url": "https://github.com/mochini/socketio-presence/issues"
  },
  "homepage": "https://github.com/mochini/socketio-presence#readme",
  "dependencies": {
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "ioredis": "^3.2.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "socket.io-redis": "^5.2.0",
    "webpack-cli": "^2.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "webpack": "^4.10.2"
  }
}
